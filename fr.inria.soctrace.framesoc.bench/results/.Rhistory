# create a summary: size, index, mean, error
dt <- data.table(read.csv("../results/temictli_bench.log"))
setkey(dt,size,index)
results = dt[,.(mean=mean(time/1000), sd=sd(time/1000), n=NROW(time/1000)), by="size,index"]
